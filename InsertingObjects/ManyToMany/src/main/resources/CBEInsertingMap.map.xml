<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/3.0/modelMap"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://cayenne.apache.org/schema/3.0/modelMap http://cayenne.apache.org/schema/3.0/modelMap.xsd"
  project-version="3.0.0.1">
	<property name="defaultPackage" value="cbe.inserting.model"/>
	<db-entity name="ROLES">
		<db-attribute name="name" type="VARCHAR" length="20"/>
		<db-attribute name="pk" type="BIGINT" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<db-entity name="USERS">
		<db-attribute name="name" type="VARCHAR" length="60"/>
		<db-attribute name="pk" type="BIGINT" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<db-entity name="USERS_TO_ROLES">
		<db-attribute name="role_fk" type="BIGINT" isMandatory="true"/>
		<db-attribute name="user_fk" type="BIGINT" isMandatory="true"/>
	</db-entity>
	<obj-entity name="Role" className="cbe.inserting.model.Role" dbEntityName="ROLES">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
	</obj-entity>
	<obj-entity name="User" className="cbe.inserting.model.User" dbEntityName="USERS">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
	</obj-entity>
	<db-relationship name="userJoin" source="ROLES" target="USERS_TO_ROLES" toMany="true">
		<db-attribute-pair source="pk" target="role_fk"/>
	</db-relationship>
	<db-relationship name="roleJoin" source="USERS" target="USERS_TO_ROLES" toMany="true">
		<db-attribute-pair source="pk" target="user_fk"/>
	</db-relationship>
	<db-relationship name="role" source="USERS_TO_ROLES" target="ROLES" toMany="false">
		<db-attribute-pair source="role_fk" target="pk"/>
	</db-relationship>
	<db-relationship name="user" source="USERS_TO_ROLES" target="USERS" toMany="false">
		<db-attribute-pair source="user_fk" target="pk"/>
	</db-relationship>
	<obj-relationship name="users" source="Role" target="User" deleteRule="Deny" db-relationship-path="userJoin.user"/>
	<obj-relationship name="roles" source="User" target="Role" deleteRule="Deny" db-relationship-path="roleJoin.role"/>
</data-map>
